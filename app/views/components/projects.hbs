{{#if resume.projects.length}}
<div class="container work-container">
  {{> title value="Projects"}}

  {{#each resume.projects}}
  <section class="item">
    {{> section-header-project name=this.name }}
    {{#if summary}}
      <p class="summary">{{summary}}</p>
    {{/if}}

    {{#if keywords.length}}
      <h5 class="specialization">
      {{#each keywords}}
          {{. }}
      {{/each}}
      </h5>
    {{/if}}
  </section>
  {{/each}}
</div>
{{/if}}
